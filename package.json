{
  "name": "earlston",
  "email": "andrew.oxenburgh@gmail.com",
  "url": "http://oh-otto.nz",
  "version": "3.8.0",
  "description": "oh-otto, an articulate bookmarker, now and then",
  "main": "./src/server.js",
  "scripts": {
    "start": ". env.sh && DEBUG=earlston:*,oh-otto:* nodemon ./src/server.js",
    "node-only": "source env.sh && node ./src/server.js",
    "postinstall": "bower install && grunt",
    "test": "./qa/test.sh",
    "doc": "pandoc doc/test-script.md -f markdown_github -t html -s -o SPECIFICATION.html",
    "lint": "./qa/jshint.sh",
    "jscs": "jscs --fix test src public/js packages/js-diff/src packages/js-diff/test",
    "jscs1": "jscs --fix public/js/oh-otto-ui.js",
    "env": "env",
    "snyk": "snyk test",
    "clean": "rm -rf ./node_modules && rm -rf ./public/bower_components",
    "qa": "npm test && npm run lint && npm run jscs && npm run snyk"
  },
